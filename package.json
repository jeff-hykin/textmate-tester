{
    "name": "textmate-tester",
    "displayName": "Textmate Tester",
    "description": "A package to help your grammars be less buggy",
    "icon": "icon.png",
    "version": "1.0.2",
    "license": "MIT",
    "bin": {
        "textmate-tester": "./index.js"
    },
    "scripts": {
        "prelint": "npm run build",
        "pretest": "npm run build",
        "test": "node test/source/index.js test",
        "testall": "npm test -- --all",
        "genall": "npm run generate-specs -- --all",
        "preversion": "npm test",
        "version": "npm run build && git add -A syntaxes",
        "_pregenerate-specs": "npm run build",
        "generate-specs": "node test/source/index.js generate-specs",
        "gen": "npm run generate-specs -- ",
        "try": "npm run test -- --show-failure-only",
        "sort-specs": "node test/source/index.js sort-specs",
        "prereport": "npm run build",
        "report": "node test/source/index.js report",
        "perf": "npm run report -- perf",
        "cov": "npm run report -- coverage"
    },
    "keywords": [
        "syntax",
        "textmate",
        "grammar"
    ],
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "repository": {
        "url": "https://github.com/jeff-hykin/textmate-tester",
        "type": "git"
    },
    "devDependencies": {},
    "dependencies": {
        "chalk": "^2.4.2",
        "convert-svg-to-png": "^0.5.0",
        "dictionary-en-us": "^2.1.1",
        "glob": "^7.1.4",
        "js-yaml": "^3.13.1",
        "json-source-map": "^0.4.0",
        "json-stable-stringify": "^1.0.1",
        "lodash": "^4.17.19",
        "minimatch": "^3.0.4",
        "node-gyp": "^4.0.0",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "prettyjson": "^1.2.1",
        "textmate-bailout": "^1.1.0",
        "uuid": "^8.3.2",
        "vscode-textmate": "^4.1.1",
        "yargs": "^16.2.0"
    }
}
